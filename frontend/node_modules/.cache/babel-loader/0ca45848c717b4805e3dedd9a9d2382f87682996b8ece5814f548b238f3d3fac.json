      });\n        console.log(response);\n        for (let i = 0; i < response.data.length; i++) {\n          console.log(i);\n          this.addAllContragents(response.data[i]);\n          console.log(response.data[i]);\n        }\n      } catch (e) {\n        alert('Error is true');\n      }\n    },\n    Show(item) {\n      console.log(item.name);\n      this.a = item.name;\n    }\n  }\n};","map":{"version":3,"names":["MyMenu","mapGetters","mapMutations","name","components","props","data","headers","dialog1","dialog2","dialog3","dialog4","a","Agents","address","inn","computed","contragents","methods","addAllContragents","addNewContragents","sendNewOrganization","res","item","console","log","createNewOrganization","response","axios","post","$store","state","url","e","alert","getOrganization","get","i","length","Show"],"sources":["src/components/MyOrganization.vue"],"sourcesContent":["<template class=\"main\">\n    <v-app>\n    \n        <section >\n            <my-menu></my-menu>\n        </section>\n        <section class=\"content\">\n            <h1>Организации-Контрагенты</h1>\n            <v-divider></v-divider>\n            <v-container >\n                <v-dialog v-model=\"dialog1\"\n                    persistent\n                    max-width=\"600px\">\n                    <template v-slot:activator=\"{ on, attrs }\">\n                        <v-btn \n                            outlined  \n                            icon color=\"blue\" \n                            v-bind=\"attrs\" \n                            v-on=\"on\"\n                            >\n                            <v-icon >mdi-plus</v-icon>\n                        </v-btn>                       \n                    </template>\n                    <v-card>\n                        <v-card-title>Добавить Организации-Контрагенты</v-card-title>\n                        <v-card-text v-for=\"(item, name, id) in Agents\">\n                            <v-text-field \n                                :name=\"name\"\n                                :key=\"id\"\n                                :placeholder=\"name\"\n                                v-model = \"Agents[name]\"                               \n                                >\n                            </v-text-field>\n                        </v-card-text>\n                        <v-card-actions>\n                            <v-spacer></v-spacer>\n                            <v-btn\n                                color=\"blue darken-1\"\n                                text\n                                @click=\"dialog2 = true\"\n                                >Закрыть\n                            </v-btn>\n                            <v-row>\n                                <v-dialog width=\"600px\" justify-center\n                                    v-model=\"dialog2\"\n                                    >\n                                    <v-card >\n                                        <v-card-title >\n                                            <div>Вы уверены, что хотите закрыть окно?</div>\n                                            <div>Все несохраненные данные будут утеряны</div>\n                                        </v-card-title>\n                                        <v-card-actions>\n                                            <v-spacer></v-spacer>\n                                                <v-btn color=\"blue darken-1\"\n                                                    text\n                                                    @click=\"dialog1 = false\"\n                                                    >Закрыть\n                                                </v-btn>\n                                                <v-btn color=\"blue darken-1\"\n                                                    text\n                                                    @click=\"dialog2 = false\"\n                                                    >Остаться\n                                                </v-btn>\n                                        </v-card-actions>\n                                    </v-card>\n                                </v-dialog>\n                            </v-row>\n                            <v-btn\n                                color=\"blue darken-1\"\n                                text \n                                @click=\"dialog1 = false, createNewOrganization(), sendNewOrganization()\"                               \n                                >Сохранить\n                            </v-btn>\n                        </v-card-actions>\n                    </v-card>\n                </v-dialog>\n            </v-container>\n            <v-divider></v-divider>\n            <v-container>\n                <v-simple-table class=\"table\">\n                    <template  v-slot:default>\n                        <thead>\n                            <th v-for=\"item in headers\" :key=\"item.text\">\n                                {{ item }}\n                            </th>\n                        </thead>\n                        <tbody>\n                            <tr v-for=\"(item, name, index) in contragents\"\n                                :key=\"item.idndex\"\n                                @click=\"Show(item)\"\n                                >\n                                <td v-for=\"value in item\">{{ value }}</td>\n                                <td>\n                                    \n                                    <v-btn \n                                        outlined  \n                                        icon color=\"blue\" >\n                                        <v-icon>mdi-pencil</v-icon>\n                                    </v-btn>          \n                                </td>\n                                <td>\n                                    <v-btn outlined  \n                                        icon color=\"red\" >\n                                        <v-icon>mdi-delete</v-icon>\n                                    </v-btn>\n                                </td>\n                            </tr>\n                        </tbody>\n                    </template>    \n                </v-simple-table>\n            </v-container>\n            {{ Agents }} {{ a }}\n        </section> \n        organization \n        <span>   \n            <router-link class=\"btn\" :to=\"{name: 'menu'}\">\n                <v-btn color=\" light\"> Назад</v-btn>\n            </router-link> \n            <v-btn @click=\"getOrganization()\">Загрузить</v-btn>\n        </span>\n    \n    </v-app>\n</template>\n\n<script>\nimport MyMenu from '../components/MyMenu.vue';\nimport { mapGetters, mapMutations } from 'vuex';\nexport default {\n    name: 'MyOrganization',\n    components: {\n        MyMenu,\n    },\n\n    props: {\n        \n    },\n\n    data() {\n        return {\n            headers:['Название', 'Адрес', 'ИНН'],\n            dialog1: false,\n            dialog2: false,\n            dialog3: false,\n            dialog4: false,\n            a: '',\n            Agents: {\n                name: '',\n                address: '',\n                inn: ''\n            }\n        }\n    },\n    computed:{\n        ...mapGetters({                      \n            contragents: \"contragents/contragent\"\n        }),\n        \n    },\n\n    methods: {\n        ...mapMutations({\n            \n            addAllContragents: 'contragents/ADD_ALL_CONTRAGENTS',\n            addNewContragents: 'contragents/ADD_NEW_CONTRAGENTS'\n        }),\n\n        sendNewOrganization() {\n            const res = {};\n            for (let item in this.Agents) {\n               res[item] = this.Agents[item];\n            }\n            this.addNewContragents(res);\n            for (let item in this.Agents) {\n               this.Agents[item] = '';\n            };\n            console.log(res)   \n        },\n\n        async createNewOrganization() {\n            try {\n                const response = await axios.post(this.$store.state.url + '/api/menu/counterparties/save',\n                {\n                    name: this.Agents.name,\n                    address: this.Agents.address,\n                    inn: this.Agents.inn\n                    \n                },\n                {headers: {\n                    \"Authorization\":  \"Bearer \" + \"eyJhbGciOiJIUzI1NiJ9.eyJzdWIiOiJhbm5ubmEifQ.8uH8-TkwiqueuYwOaA7VpFpjQZyvXZbgJWyBc99tN2E\",\n                }});\n                console.log(response)\n                \n            } \n            catch(e) {\n                alert('Неверно')\n                }\n\n        },\n        async getOrganization() {\n            try {\n                const response = await axios.get(this.$store.state.url + '/api/menu/counterparties/show', \n                {headers: {\n                    \"Authorization\":  \"Bearer eyJhbGciOiJIUzI1NiJ9.eyJzdWIiOiJhbm5ubmEifQ.8uH8-TkwiqueuYwOaA7VpFpjQZyvXZbgJWyBc99tN2E\",\n                    \"Cache-Control\": null,\n                    \"X-Requested-With\": null                   \n                }})\n                console.log(response)\n                for (let i = 0; i<response.data.length; i++)  {\n                    console.log(i);\n                    this.addAllContragents(response.data[i])\n                    console.log(response.data[i]);                   \n                }                         \n            } \n            catch(e) {\n                alert('Error is true')\n            }      \n        },\n\n        Show(item) {\n            console.log(item.name)\n            this.a=item.name\n        },\n    }\n}\n</script>\n\n\n\n<style  scoped>\n.main {\n    max-height: 400px;  \n    max-width: 250px; \n}\nh1 {\n    text-align: center;\n}\n.content {\n    padding-left: 280px;\n    padding-top: 20px;\n    position: fixed;\n}\n.btn {\n    margin-left:20px\n}\n</style>"]